{
  "name": "shipberth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:socket": "ts-node server.js",
    "build": "next build",
    "build:socket": "next build && tsc --project tsconfig.server.json",
    "start": "next start",
    "start:socket": "cross-env NODE_ENV=production pm2 start dist/server.js --name ship-birth-server && pm2 logs ship-birth-server",
    "stop:socket": "pm2 stop ship-birth-server",
    "restart:socket": "pm2 restart ship-birth-server",
    "delete:socket": "pm2 delete ship-birth-server",
    "lint": "next lint"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@tanstack/react-query": "^5.69.0",
    "antd": "^5.25.1",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "mime": "^4.0.7",
    "mssql": "^11.0.1",
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.86.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/mssql": "^9.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.0",
    "cross-env": "^7.0.3",
    "typescript": "^5"
  }
}
